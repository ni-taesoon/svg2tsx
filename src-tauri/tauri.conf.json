{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Svg2Tsx",
  "version": "0.2.0",
  "identifier": "com.taesoonpark.svg2tsx",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Svg2Tsx",
        "width": 640,
        "height": 904,
        "minWidth": 640,
        "minHeight": 398,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "backgroundColor": [
          47,
          47,
          47,
          255
        ],
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "DeveloperTool",
    "shortDescription": "SVG를 React TSX 컴포넌트로 변환",
    "longDescription": "SVG2TSX는 SVG 파일을 React TSX 컴포넌트로 변환하는 데스크톱 애플리케이션입니다. 드래그 앤 드롭으로 간편하게 SVG를 가져오고, 다양한 옵션으로 TSX 컴포넌트를 생성할 수 있습니다.",
    "copyright": "Copyright © 2024 Taesoon Park",
    "macOS": {
      "entitlements": null,
      "minimumSystemVersion": "10.15"
    },
    "windows": {
      "digestAlgorithm": "sha256",
      "wix": null,
      "nsis": {
        "installMode": "currentUser"
      }
    },
    "linux": {
      "appimage": {
        "bundleMediaFramework": false
      },
      "deb": {
        "section": "devel"
      }
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDY0RjQxQzg2RjY5NjFFM0QKUldROUhwYjJoaHowWkF4UmVqMXdsNmNmTU5UYzQ2Z3NqK0V3T1pCRWROUG9yU3phV3VWWGtoanoK",
      "endpoints": [
        "https://github.com/ni-taesoon/svgtsx-release/releases/latest/download/latest.json"
      ]
    }
  }
}
